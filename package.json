{
  "name": "nextjango",
  "version": "0.1.0",
  "description": "CLI to scaffold fullstack Next.js + Django projects",
  "author": {
    "name": "Sylken",
    "url": "https://sylken.io"
  },
  "type": "module",
  "bin": {
    "nextjango": "./dist/src/bin/nextjango.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc && node scripts/copy-templates.js && pnpm inject-shebang",
    "inject-shebang": "node scripts/inject-shebang.js",
    "link": "pnpm link --global",
    "nj": "nextjango",
    "copy-templates": "cpy \"templates/**\" dist --cwd=."
  },
  "keywords": [
    "cli",
    "fullstack",
    "nextjs",
    "django",
    "scaffold",
    "boilerplate",
    "generator",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylkenio/nextjango"
  },
  "bugs": {
    "url": "https://github.com/sylkenio/nextjango/issues"
  },
  "homepage": "https://github.com/sylkenio/nextjango#readme",
  "preferGlobal": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.30",
    "cpy": "^11.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
